{
    "version": 2,
    "builds": [
      { "src": "login-project/server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/register", "dest": "/login-project/server.js" },
      { "src": "/login", "dest": "/login-project/server.js" },
      { "src": "/", "dest": "/site/home.html" },
      { "src": "/site/(.*)", "dest": "/site/$1" }
    ]
  }
  